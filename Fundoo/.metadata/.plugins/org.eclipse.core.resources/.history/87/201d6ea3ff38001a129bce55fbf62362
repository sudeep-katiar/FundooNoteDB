package com.blblz.fundoonotes.model;

import java.util.List;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToMany;
import javax.persistence.ManyToOne;
import javax.persistence.Table;
import javax.validation.constraints.NotBlank;

@Entity
@Table(name = "label")
public class LabelModel {
	
	@Id
	private String labelId;
	
	@NotBlank
	private String labelTitle;
	
	@ManyToOne
	@JoinColumn(name = "userId")
	private UserModel userLabel;

	@ManyToMany(mappedBy = "labels")
	private List<NoteModel>listnote;
	
	public LabelModel() 
	{
		
	}

	public LabelModel(String labelId, @NotBlank String labelTitle, UserModel userLabel, List<NoteModel> listnote) {
		super();
		this.labelId = labelId;
		this.labelTitle = labelTitle;
		this.userLabel = userLabel;
		this.listnote = listnote;
	}

	public LabelModel(String labelId, @NotBlank String labelTitle) {
		super();
		this.labelId = labelId;
		this.labelTitle = labelTitle;
	}

	public String getLabelId() {
		return labelId;
	}

	public void setLabelId(String labelId) {
		this.labelId = labelId;
	}

	public String getLabelTitle() {
		return labelTitle;
	}

	public void setLabelTitle(String labelTitle) {
		this.labelTitle = labelTitle;
	}

	public UserModel getUserLabel() {
		return userLabel;
	}

	public void setUserLabel(UserModel userLabel) {
		this.userLabel = userLabel;
	}

	public List<NoteModel> getListnote() {
		return listnote;
	}

	public void setListnote(List<NoteModel> listnote) {
		this.listnote = listnote;
	}

}
